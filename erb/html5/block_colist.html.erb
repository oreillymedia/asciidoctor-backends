<%#encoding:UTF-8%><div<%= @id && %( id="#{@id}") %> class="colist<%= attr?('style') ? %( #{attr 'style'}) : nil %><%= attr?('role') ? %( #{attr 'role'}) : nil %>"><%= title? ? %(
<div class="title">#{title}</div>) : nil %><%
if attr? 'icons' %>
<table><% items.each_with_index do |item, i| %>
<tr>
<td><img src="<%= icon_uri("callouts/#{i + 1}") %>" alt="<%= i + 1 %>"></td>
<td><%= item.text %></td>
</tr><% end %>
</table><%
else %>
<ol><% items.each do |item| %>
<li>
<p><%= item.text %></p><% if item.blocks? %>
<%= item.content %><% end %>
</li><% end %>
</ol><% end %>
</div>
